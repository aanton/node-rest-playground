{
  "name": "node-rest-playground",
  "version": "1.0.0",
  "description": "Playground project for develop a REST API in NodeJS",
  "repository": {
    "type": "git",
    "url": "https://github.com/aanton/node-rest-playground.git"
  },
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon -r dotenv/config --exec babel-node src/index.js",
    "seed": "babel-node -r dotenv/config src/seed.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "aanton",
  "license": "ISC",
  "dependencies": {
    "body-parser": "1.20.1",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "lodash.random": "3.2.0",
    "lodash.samplesize": "4.2.0",
    "morgan": "1.10.0",
    "sequelize": "6.29.0",
    "sqlite3": "5.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@babel/node": "7.20.2",
    "@babel/preset-env": "7.20.2",
    "@ngneat/falso": "6.2.0",
    "jest": "29.3.1",
    "nodemon": "2.0.20",
    "supertest": "6.3.1"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "dotenv/config"
    ],
    "setupFilesAfterEnv": [
      "./tests/setup"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
